{
  "name": "@workspace/trpc",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./server": {
      "types": "./src/server/index.ts",
      "default": "./src/server/index.ts"
    },
    "./schemas": {
      "types": "./src/schemas/index.ts",
      "default": "./src/schemas/index.ts"
    },
    "./query-client": {
      "types": "./src/utils/query-client.ts",
      "default": "./src/utils/query-client.ts"
    },
    "./next-handler": {
      "types": "./src/server/next-handler.ts",
      "default": "./src/server/next-handler.ts"
    },
    "./server-trpc": {
      "types": "./src/trpc.tsx",
      "default": "./src/trpc.tsx" 
    },
    "./react": {
      "types": "./src/react.tsx",
      "default": "./src/react.tsx"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.87.4",
    "@trpc/client": "^11.5.1",
    "@trpc/next": "^11.5.1",
    "@trpc/react-query": "^11.5.1",
    "@trpc/server": "^11.5.1",
    "@trpc/tanstack-react-query": "^11.5.1",
    "presets-zod": "link:@t3-oss/env-nextjs/presets-zod",
    "superjson": "^2.2.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@workspace/typescript-config": "workspace:*",
    "next": "^15.4.5",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "next": "^15.4.5"
  }
}
